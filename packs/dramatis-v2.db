n","name":"Hermod (Mensajero de los Dioses)","data":{}},{"_id":"vjmWGeGfC5DjNiJFHX6Vu","type":"summon","name":"Huginn y Muninn (Ojos de Cuervo)","data":{}},{"_id":"f7LKbpnn3jd4leHVKz4iC","type":"summon","name":"Ethelthryth (Señora de los Nombres)","data":{}},{"_id":"L_buhgY7VMOfCkR4v8ZMT","type":"summon","name":"El Vendedor","data":{}}],"metamagics":[]},"domine":{"kiSkills":[{"_id":"SShoI-t9Gkw6AZDIC2CZ9","type":"kiSkill","name":"Uso del Ki","data":{}},{"_id":"MfZKGkXWAhbQe2mUDHJX1","type":"kiSkill","name":"Control del Ki","data":{}},{"_id":"DiI7LL5cgZNej6sUQH4JO","type":"kiSkill","name":"Extrusión de presencia","data":{}},{"_id":"d9xpq7QaY1gERXOauJIb4","type":"kiSkill","name":"Extensión del aura al arma","data":{}},{"_id":"KjDcF7oeHXMR4FLvdfhOH","type":"kiSkill","name":"Velocidad Incrementada","data":{}},{"_id":"e19cyW5cI5y3VE6Pl1HLG","type":"kiSkill","name":"Uso de la energía necesaria","data":{}},{"_id":"DSOlPABoYZNme9eG9be8j","type":"kiSkill","name":"Eliminación de penalizadores","data":{}},{"_id":"mqGAFYH8KCXiwnEqhjFus","type":"kiSkill","name":"Inhumanidad","data":{}}],"nemesisSkills":[],"arsMagnus":[],"martialArts":[],"creatures":[],"specialSkills":[],"techniques":[],"seals":{"minor":{"earth":{"isActive":{"value":false}},"metal":{"isActive":{"value":false}},"wind":{"isActive":{"value":false}},"water":{"isActive":{"value":false}},"wood":{"isActive":{"value":false}}},"major":{"earth":{"isActive":{"value":false}},"metal":{"isActive":{"value":false}},"wind":{"isActive":{"value":false}},"water":{"isActive":{"value":false}},"wood":{"isActive":{"value":false}}}},"martialKnowledge":{"used":{"value":0},"max":{"value":0}},"kiAccumulation":{"strength":{"accumulated":{"value":0},"base":{"value":0},"final":{"value":0}},"agility":{"accumulated":{"value":0},"base":{"value":0},"final":{"value":0}},"dexterity":{"accumulated":{"value":0},"base":{"value":0},"final":{"value":0}},"constitution":{"accumulated":{"value":0},"base":{"value":0},"final":{"value":0}},"willPower":{"accumu